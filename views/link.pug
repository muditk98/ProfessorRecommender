html
	head
		title="Professor Rating System"
		link(rel='stylesheet',href='/stylesheets/style.css')
	body
		div
			each prof in profs
				a(href=prof.link)
					p #{prof.name} #{prof.school} 
					ul
						each cid in prof.courses.map(course => course.cid)
							li= cid
			if !profs.length
				p No professors found
